{
  "name": "phpunitgen/core",
  "description": "The PhpUnitGen core features for tests generation.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "test",
    "generation",
    "generator"
  ],
  "support": {
    "issues": "https://github.com/paul-thebaud/phpunitgen-core/issues",
    "source": "https://github.com/paul-thebaud/phpunitgen-core"
  },
  "authors": [
    {
      "name": "Paul Thébaud",
      "email": "paul.thebaud29@gmail.com"
    },
    {
      "name": "Killian Hascoët",
      "email": "killianh@live.fr"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "league/container": "^3.2",
    "phpdocumentor/reflection-docblock": "^4.0|^5.0",
    "ondrejmirtes/better-reflection": ">=4.3.51,<5.0.0",
    "tightenco/collect": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^7.3|^8.5|^9.1"
  },
  "autoload": {
    "psr-4": {
      "PhpUnitGen\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\PhpUnitGen\\Core\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
